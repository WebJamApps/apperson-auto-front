  "scripts": {
    "start": "nps",
    "test": "npm run test:karma && nps e2e",
    "test:karma": "cross-env NODE_ENV=test nps test.lint && nps test.karma",
    "cleaninstall": "rm -rf bower_components && rm -rf yarn.lock && rm -rf node_modules && npm i -g bower && npm i -g yarn && yarn cache clean && yarn install && npm run build:polymer && npm run build:prod",
    "build:polymer": "bower install && npm run vulc",
    "vulc": "rm -rf static/includes.html && polymer-bundler --inline-scripts --inline-css includes.html > static/includes.html",
    "server:prod": "pushstate-server dist",
    "build:prod": "npm start -- build",
    "prod": "npm run build:prod && npm run server:prod",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "eslint-fix": "./node_modules/eslint/bin/eslint.js --fix ."
  },
